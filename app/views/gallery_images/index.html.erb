<p id="notice"><%= notice %></p>

<h1>Gallery Images</h1>

<table>
  <thead>
    <tr>
      <th>Gallery</th>
      <th>Image</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gallery_images.each do |gallery_image| %>
      <tr>
        <td><%= gallery_image.gallery_id %></td>
        <td><%= gallery_image.image %></td>
        <td><%= link_to 'Show', gallery_image %></td>
        <td><%= link_to 'Edit', edit_gallery_image_path(gallery_image) %></td>
        <td><%= link_to 'Destroy', gallery_image, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gallery Image', new_gallery_image_path %>
